<hr class="my-5">
<div class="card text-center text-bg-success">
    {{#with service}}
    <h2> {{title}} </h2>
    <p> {{description}} </p>
    {{#each buttons}}
    <div class="card-body text-center">
        <a href="{{link}}" type="button" class="btn btn-primary col-2 ">{{text}} </a>
        {{/each}}
        {{/with}}
    </div>