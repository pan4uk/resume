{{#each data}}
<ul class="list-group">
    <li class="list-group-item"><strong>{{name}}</strong>({{id}})</li>
    <li class="list-group-item">{{gender}}</li>
    <li class="list-group-item">{{age}}</li>
    <li class="list-group-item">{{> users data=friends}}</li>
    <li class="list-group-item">{{> users data=mutual_friends}}</li>
    <li class="list-group-item">{{> groups data=groups}}</li>
</ul>
{{/each}}