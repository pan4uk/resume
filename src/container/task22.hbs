<main>
    <div>
        <h1 class="text-center">{{page_header.title}}</h1>
        <div class="text-center text-uppercase">
            {{page_header.description}}
        </div>
    </div>
    <br>
    <div>
        {{#with section}}
        <div class="card">
            <div class="card-body">
                {{#with about_us}}
                <div class="text-end display-6">
                    {{title_list.main}}
                </div>
                <div class="text-end">
                    <h2>{{title_list.sub}}</h2>
                </div>

                <button type="button" class="btn btn-outline-info">{{button.text}}</button>
                {{/with}}
            </div>
        </div>
        <br>
        <div class="card">
            <div class="card-body">
                {{#with advantage}}
                <div class="text-muted">
                    <div class="display-5"> {{title}} </div>
                </div>
                <br>
                <div>
                    {{#with efficiency}}
                    <div class="card">
                        <div class="card-body">
                            <div class="display-4 text-muted"> {{title}} </div>
                            <div>
                                <h6> {{sub_title}} </h6>
                            </div>
                            <div>
                                <p>
                                    {{description}}
                                </p>
                            </div>
                            <button type="button" class="btn btn-outline-warning">{{button}}</button>
                        </div>
                    </div>
                    {{/with}}
                </div>
                <br>
                <div>
                    {{#with innovation}}
                    <div class="card">
                        <div class="card-body">
                            <div class="display-4 text-muted text-center"> {{title}} </div>
                            <div>
                                <h6 class="text-center"> {{sub_title}} </h6>
                            </div>
                            <div>
                                <p class="text-center">
                                    {{description}}
                                </p>
                            </div>
                            <button type="button" class="btn btn-outline-warning">{{button}}</button>
                        </div>
                    </div>
                    {{/with}}
                </div>
                <br>
                <div>
                    {{#with collaboration}}
                    <div class="card">
                        <div class="card-body">
                            <div class="display-4 text-muted text-end"> {{title}} </div>
                            <div>
                                <h6 class="text-end"> {{sub_title}} </h6>
                            </div>
                            <div>
                                <p class="text-end">
                                    {{description}}
                                </p>
                            </div>
                            <button type=" button" class="btn btn-outline-warning">{{button}}</button>
                        </div>
                    </div>
                    {{/with}}
                </div>
                {{/with}}
            </div>
        </div>
        <br>
        <div class="card">
            {{#with agreement}}
            <div class="card-body">
                <div class="display-5">
                    {{title}}
                </div>
                <br>
                <div class="card">
                    {{#with info}}
                    <div class="card-body">
                        <h5>{{sub_title}}</h5>
                        <div class="card">
                            <div class="card-body">
                                <p> {{detail.text}} </p>
                                <br>
                                <button type="button" class="btn btn-success">{{detail.button}} </button>
                            </div>
                        </div>
                    </div>
                    {{/with}}
                </div>
            </div>
            {{/with}}
        </div>
        {{/with}}
    </div>
</main>