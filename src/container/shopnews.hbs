<main class="row m-3">
    <header>
        {{> shopheader}}
    </header>

    <body>
        <div>
            {{> shopbreadcrumb}}
        </div>
        <div>
            {{#with newsData}}
            <div class="card">
                <img src="{{photo}}">
            </div>
            <div>
                {{#with head}}
                <header>
                    <h1 class="mt-2">{{heading}}</h1>
                    <div> {{description}} </div>
                </header>
                {{/with}}
                {{#each body}}

                <body class="mt-2">
                    <h2 class="mt-2"> {{title}} </h2>
                    <div> {{text}} </div>
                </body>
                {{/each}}
                {{/with}}
            </div>
            <hr class="my-5">
            <div>
                {{#with newsOtherBlock}}
                <h2 class="text-center"> {{title}} </h2>
                <div class="row">
                    {{#each cards}}
                    {{> shoppost}}
                    {{/each}}
                </div>
                {{/with}}
            </div>

            <div>
                {{> shopaction}}
            </div>
        </div>
    </body>

    <footer>
        {{> shopfooter}}
    </footer>

</main>




























